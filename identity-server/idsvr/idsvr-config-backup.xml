                <refresh-token-ttl>disabled</refresh-token-ttl>
                <scope>openid</scope>
                <scope>read</scope>
                <user-authentication>
                  <allowed-authenticators>UsernamePassword</allowed-authenticators>
                </user-authentication>
                <allowed-origins>https://oauth.tools</allowed-origins>
                <allowed-origins>https://login.example.com</allowed-origins>
                <capabilities>
                  <code/>
                  <client-credentials/>
                  <haapi/>
                </capabilities>
                <use-pairwise-subject-identifiers>
                  <sector-identifier>haapi-web-client</sector-identifier>
                </use-pairwise-subject-identifiers>
                <validate-port-on-loopback-interfaces>true</validate-port-on-loopback-interfaces>
                <attestation>
                  <disable-attestation-validation>false</disable-attestation-validation>
                  <web>
                    <web-policy>web-policy</web-policy>
                  </web>
                </attestation>
              </client>
              <client>
                <id>scim-client</id>
                <client-name>scim-client</client-name>
                <secret>$5$bf8/8FWHVh864roK$S7Yx5UdFatfwHYSEhmN34oGo6BG0uspx3/P6R3gCoN5</secret>
                <scope>read</scope>
